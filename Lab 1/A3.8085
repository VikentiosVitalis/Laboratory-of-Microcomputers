		LXI B,01F4H	
		IN 20H
		ANI 01H
		MOV E,A	
		MVI A,FEH	
		OUT 30H
		CALL DELB
		
LEFT:		RLC		
		OUT 30H	
		CALL DELB
		CALL CHECK	
		MOV D,A
		MOV A,H
		CPI 00H	 
		MOV A,D	
		JZ LEFT	
		JMP RIGHT	
				
RIGHT:		RRC	
		OUT 30H
		CALL DELB
		CALL CHECK	
		MOV D,A
		MOV A,H
		CPI 00H	
		MOV A,D
		JZ RIGHT	
		JMP LEFT	
		
CHECK:		PUSH PSW
		MOV L,A
STATIC:	IN 20H		
		MOV D,A
		RAL
		JNC STATIC
		MOV A,L
		MVI H,00H	
		CPI 7FH	
		JNZ C2
		MVI H,01H	
		CALL DELB	
		JMP EXIT	
C2:		CPI FEH	
		JNZ CONT
		MVI H,01H	
		JMP EXIT	

CONT:		MOV A,D	
		ANI 01H
		MOV L,A	
		XRA E		
		MOV E,L	
		MOV H,A

EXIT:		POP PSW
		RET	

		END